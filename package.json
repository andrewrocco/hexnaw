{
  "name": "hexnaw-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_PATH=src next",
    "build": "NODE_PATH=src next build",
    "start": "NODE_PATH=src next start",
    "test": "NODE_ENV=test NODE_PATH=src jest",
    "test:watch": "NODE_ENV=test NODE_PATH=src jest --watch",
    "test:coverage": "NODE_ENV=test NODE_PATH=src jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "jest-plugin-context/setup",
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "colorable": "^1.0.5",
    "formik": "^1.5.0",
    "next": "^9.3.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-ga": "^2.7.0",
    "reflexbox": "^4.0.6",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^25.1.0",
    "jest-plugin-context": "^2.9.0",
    "jest-styled-components": "^7.0.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.13.0"
  }
}
